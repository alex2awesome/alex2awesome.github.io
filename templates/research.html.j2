<script>
    $(document).ready(function () {
        function getUrlParameter(name) {
            name = name.replace(/[\[]/, '\\[').replace(/[\]]/, '\\]');
            var regex = new RegExp('[\\?&]' + name + '=([^&#]*)');
            var results = regex.exec(location.search);
            return results === null ? '' : decodeURIComponent(results[1].replace(/\+/g, ' '));
        }

        var lensParam = getUrlParameter('lens');
        var activeLens = lensParam === 'cs' ? 'cs' : 'cj';

        if (activeLens === 'cs') {
            $('#lens').text('Computer Science Lens');
            $('#computer-science-lens').show();
            $('#comp-journalism-lens').hide();
        } else {
            $('#lens').text('Computational Journalism Lens');
            $('#computer-science-lens').hide();
            $('#comp-journalism-lens').show();
        }

        var activeCsFilter = null;
        var activeCjFilter = null;
        var $publicationsHeader = $('#publications-header');
        var basePublicationsHeader = $publicationsHeader.text();

        var csFilterLabels = {
            actions: 'Actions',
            discourse: 'Discourse Elements',
            words: 'Words'
        };

        var cjFilterLabels = {
            'story-idea': 'Story Idea',
            'article-in-progress': 'Article-in-progress',
            'finished-story': 'Finished Story',
            'sourced-story': 'Sourced Story',
            'structured-story': 'Structured Story'
        };

        var csGroups = {
            actions: [
                'Do LLMs Plan Like Human Writers?',
                'Tracking the Newsworthiness of Public Documents',
                'Explaining Mixtures of Sources in News Articles',
                'First Steps Towards a Source Recommendation Engine',
                'A Novel Multi-Document Retrieval Benchmark',
                'NewsInterview: a Dataset and a Playground to Evaluate LLMs',
                'Identifying Informational Sources in News Articles',
                "Don't quote me on that: Finding Mixtures of Sources in News Articles",
                'Modeling Newsworthiness for Lead-Generation Across Corpora'
            ],
            discourse: [
                'LegalDiscourse: Interpreting When Laws Apply and To Whom',
                'Multitask Semi-Supervised Learning for Class-Imbalanced Discourse Classification',
                'News Discourse Patterns: A Roadmap for Computational Journalism',
                'StateCensusLaws.org: A Web Application for Consuming and Annotating Legal Discourse Learning',
                'Understanding multimodal procedural knowledge by sequencing multimodal instructional manuals',
                'PatentEdits: Framing Patent Novelty as Textual Entailment'
            ],
            words: [
                'Stay on Topic with Classifier-Free Guidance',
                'NewsEdits: A News Article Revision Dataset and a Novel Document-level Reasoning Challenge',
                'NewsEdits 2.0: Learning the Intentions Behind Updating News',
                'Enabling Low-Resource Transfer Learning across COVID-19 Corpora by Combining Event-Extraction and Co-Training',
                'Characterizing the Internet Research Agency‚Äôs Social Media Operations During the 2016 US Presidential Election using Linguistic Analyses',
                'Actionable Recourse in Linear Classification. (Expanded Version)',
                'Actionable Recourse in Linear Classification'
            ]
        };

        var cjGroups = {
            'story-idea': [
                'Tracking the Newsworthiness of Public Documents',
                'Modeling Newsworthiness for Lead-Generation Across Corpora',
                'A Novel Multi-Document Retrieval Benchmark',
                'Spatial Layouts in News Homepages Capture Human Preferences',
                'Characterizing Search Engine Traffic to Internet Research Agency Web Properties'
            ],
            'article-in-progress': [
                'Do LLMs Plan Like Human Writers?',
                'NewsEdits: A News Article Revision Dataset and a Novel Document-level Reasoning Challenge',
                'NewsEdits 2.0: Learning the Intentions Behind Updating News',
                'Stay on Topic with Classifier-Free Guidance',
                'Are Large Language Models Capable of Generating Human-Level Narratives?',
                'PatentEdits: Framing Patent Novelty as Textual Entailment'
            ],
            'finished-story': [
                'LegalDiscourse: Interpreting When Laws Apply and To Whom',
                'News Discourse Patterns: A Roadmap for Computational Journalism',
                'Understanding multimodal procedural knowledge by sequencing multimodal instructional manuals',
                'Characterizing the Internet Research Agency‚Äôs Social Media Operations During the 2016 US Presidential Election using Linguistic Analyses',
                'StateCensusLaws.org: A Web Application for Consuming and Annotating Legal Discourse Learning'
            ],
            'sourced-story': [
                'Explaining Mixtures of Sources in News Articles',
                'Identifying Informational Sources in News Articles',
                'First Steps Towards a Source Recommendation Engine',
                "Don't quote me on that: Finding Mixtures of Sources in News Articles",
                'NewsInterview: a Dataset and a Playground to Evaluate LLMs',
                'A Novel Multi-Document Retrieval Benchmark'
            ],
            'structured-story': [
                'Multitask Semi-Supervised Learning for Class-Imbalanced Discourse Classification',
                'LegalDiscourse: Interpreting When Laws Apply and To Whom',
                'StateCensusLaws.org: A Web Application for Consuming and Annotating Legal Discourse Learning',
                'Understanding multimodal procedural knowledge by sequencing multimodal instructional manuals',
                'PatentEdits: Framing Patent Novelty as Textual Entailment',
                'News Discourse Patterns: A Roadmap for Computational Journalism',
                'Spatial Layouts in News Homepages Capture Human Preferences'
            ]
        };

        var $paperEntries = $('#cs-papers > div[valign="top"]').addClass('paper-entry');

        function findPaper(titleFragment) {
            return $('.paper_title')
                .filter(function () {
                    return $(this).text().indexOf(titleFragment) !== -1;
                })
                .closest('div[valign]');
        }

        function assignGroups(groupMap, lensKey) {
            $.each(groupMap, function (group, titles) {
                titles.forEach(function (titleFragment) {
                    var $matches = findPaper(titleFragment);
                    if (!$matches.length) {
                        return;
                    }
                    var attributeName = lensKey === 'cs' ? 'data-cs' : 'data-cj';
                    $matches.each(function () {
                        var $entry = $(this);
                        var existing = ($entry.attr(attributeName) || '').split(/\s+/).filter(function (token) {
                            return token.length > 0;
                        });
                        if (existing.indexOf(group) === -1) {
                            existing.push(group);
                        }
                        $entry.attr(attributeName, existing.join(' '));
                    });
                });
            });
        }

        assignGroups(csGroups, 'cs');
        assignGroups(cjGroups, 'cj');

        function updateHeaderForFilter(lensKey, filterValue) {
            if (lensKey !== activeLens) {
                return;
            }
            if (!filterValue) {
                $publicationsHeader.text(basePublicationsHeader);
                return;
            }
            var labelMap = lensKey === 'cs' ? csFilterLabels : cjFilterLabels;
            var label = labelMap[filterValue] || filterValue;
            $publicationsHeader.text(basePublicationsHeader + ' (' + label + ')');
        }

        function applyFilter(lensKey, filterValue) {
            $paperEntries.removeClass('paper-hidden');
            if (filterValue) {
                var attributeName = lensKey === 'cs' ? 'data-cs' : 'data-cj';
                $paperEntries.each(function () {
                    var groups = ($(this).attr(attributeName) || '').split(/\s+/).filter(function (token) {
                        return token.length > 0;
                    });
                    if (groups.indexOf(filterValue) === -1) {
                        $(this).addClass('paper-hidden');
                    }
                });
            }
            updateHeaderForFilter(lensKey, filterValue);
        }

        function handleFilterMessage(lensKey, value) {
            if (lensKey === 'cs') {
                activeCsFilter = value || null;
                if (activeLens === 'cs') {
                    applyFilter('cs', activeCsFilter);
                }
            } else {
                activeCjFilter = value || null;
                if (activeLens === 'cj') {
                    applyFilter('cj', activeCjFilter);
                }
            }
        }

        applyFilter(activeLens, activeLens === 'cs' ? activeCsFilter : activeCjFilter);

        window.addEventListener('message', function (event) {
            if (!event || typeof event.data !== 'object' || event.data === null) {
                return;
            }
            if (event.data.type === 'cs-filter') {
                handleFilterMessage('cs', event.data.value);
            } else if (event.data.type === 'cj-filter') {
                handleFilterMessage('cj', event.data.value);
            }
        });
    });
</script>

